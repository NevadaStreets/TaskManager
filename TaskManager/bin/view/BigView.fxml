                        <MenuItem mnemonicParsing="false" onAction="#razon" text="Relacion tiempo-tareas" />
                    </items>
                  </Menu>
                  <MenuItem fx:id="Email" mnemonicParsing="false" onAction="#isOkClicked" text="Email" />
